<!DOCTYPE html>
<html>
<head>
  <title>JulySoundcheck</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <% if current_user %>
    Hello, <%=link_to current_user.displayname, current_user%>
    <%= link_to "Log Out", logout_path %>
  <% else %>
    <b><%= link_to "Sign up", signup_path %></b>
    <%= link_to "Log in", login_path %>
  <% end %>
</head>
<body>

<%= yield %>

</body>
</html>
